{
    "restApiRoot": "/api",
    "host": "0.0.0.0",
    "port": 3000,
    "cookieSecret": "cookie-secret",
    "remoting": {
        "context": false,
        "rest": {
            "normalizeHttpPath": false,
            "xml": false
        },
        "json": {
            "strict": false,
            "limit": "2048kb"
        },
        "urlencoded": {
            "extended": true,
            "limit": "2048kb"
        },
        "cors": false,
        "errorHandler": {
            "disableStackTrace": false
        }
    },
    "legacyExplorer": false,
    "log": {
        "type": "rotating-file",
        "path": "./fw-log.log",
        "period": "1d",
        "count": 3
    },
    "frameworkdsname": "db",
    "systemId": "temporaryId",
    "disablecaching": false,
    "modelstocache": [
        "BaseACL",
        "ModelDefinition",
        "AppConfig"
    ],
    "tenantsource": "HEADER",
    "tenantkey": "tenant_id",
    "app": "oecloud.io",
    "realm": "oecloud",
    "encryptionAlgorithm": "crypto.aes256",
    "encryptionPassword": "SomePassword",
    "defaultAutoScope": "default",
    "MAX_OTP_FAILED_ATTEMP": 3,
    "DEFAULT_TTL_OTP": 180000,
    "UNLOCK_USER_ACCOUNT_TIME": 600000,
    "denyUnauthenticatedWrite": true,
    "maxFileSize": 4000,
    "supportedFileExtns": [],
    "fileNamePattern": "^[a-zA-Z0-9\\_\\-]+$",
    "passwordComplexity": {
        "regex": "",
        "errMsg": "Password complexity not met. Password length should be minimum 8 and maximum 20.Password should contain atleast one capital, one small letter, one numaric and one special character(+$@$!%*#?&)."
    },
    "allowedHTTPMethods": [
        "GET",
        "POST",
        "PUT",
        "DELETE",
        "PATCH"
    ],
    "maxFailedLoginTries": 5,
    "memoryInterval": 1000,
    "memoryPoolChunk": 3,
    "eventReliabilityReplayThreshold": 3000,
    "eventReliabilityReplayInterval": 2000,
    "eventReliabilityMaxRetryInterval": 300000,
    "eventReliabilityMaxFetch": 1000,
    "eventReliabilityServiceWindowStart": 360000,
    "eventReliabilityServiceWindowEnd": 14400000,
    "maxMessagesToProcess": 100,
    "intervalActorCanBeDirty": 600,
    "enableDesigner": true,
    "designer": {
        "installationPath": "client/bower_components",
        "mountPath": "/designer",
        "templatePath": [],
        "modules": []
    },
    "metadataModelList": {
        "ModelDefinition": true,
        "UIComponent": true,
        "UIRoute": true,
        "UIItem": true,
        "NavigationLink": false,
        "UIElement": false,
        "PersonalizationRule": true
    },
    "memoryPoolSize": 10000,
    "ruleEngine": "Drools",
    "jsFeelRelation": {
        "disabled": false
    },
    "jsFeelExternalFunction": {
        "disabled": false,
        "path": ""
    },
    "defaultUser": {
        "userName": "admin",
        "password": "admin",
        "email": "admin@mycompany.com"
    },
    "dbLockMode": "dbLock",
    "dbLockDuration": 10000,
    "scriptTag": [
        "<img",
        "<a href",
        "<input",
        "<iframe",
        "<link"
    ],
    "serverMonitor": {}
}