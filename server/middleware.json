{
    "initial:before": {
        "loopback#favicon": {}
    },
    "initial": {
        "compression": {},
        "cors": {
            "params": {
                "origin": true,
                "credentials": true,
                "maxAge": 86400
            }
        }
    },
    "session:before": {
        "loopback#context": {},
        "./middleware/pre-auth-context-populator": {
            "params": {
                "excludeHeadersList": [
                    "host",
                    "accept-encoding",
                    "accept",
                    "content-type",
                    "content-length",
                    "connection",
                    "user-agent",
                    "x-jwt-assertion",
                    "cookie",
                    "if-none-match"
                ],
                "queryStringContext": [
                    "device-type",
                    "location",
                    "language",
                    "tenant-id"
                ]
            }
        }
    },
    "session": {},
    "session:after": {},
    "auth": {
        "./middleware/jwt-assertion": {
            "enabled": true
        }
    },
    "auth:after": {
        "./middleware/post-auth-context-populator": {}, 
        "./middleware/node-red-access-control": {}
    },
    "parse": {},
    "routes:before": {
        "./middleware/http-method-overriding-filter": {},
        "./middleware/req-logging-filter": {},
        "./middleware/useragent-populator-filter": {},
        "./middleware/model-discovery-filter": {
            "enabled": true,
            "paths": ["${restApiRoot}"]
        },
        "./middleware/otp-middleware": {
            "enabled": true,
            "paths": ["${restApiRoot}"]
        },
        "./middleware/recaptcha-validate": {},
        "./middleware/db-lock-contributor": {}
    },
    "routes": {
    },
    "files": {
        "loopback#static": {
            "params": "$!../client"
        }
    },
    "final": {
        "./middleware/url-not-found": {}
    },
    "final:after": {
        "errorhandler": {}
    }
}
