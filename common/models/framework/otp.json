{
  "name": "OTP",
  "base": "PersistedModel",
  "idInjection": false,
  "description": "This model is used in OTP functionality",
  "options": {
    "isFrameworkModel": true,
    "validateUpsert": true
  },
  "properties": {
    "token": {
      "type": "number",
      "required": true
    },
    "timestamp": {
      "type": "number",
      "required": true
    },
    "state": {
      "type": "string",
      "required": true,
      "max": 30
    },
    "data": {
      "type": "object",
      "required": false
    },
    "failedAttemps": {
      "type": "number",
      "default": 0
    }
  },
  "hidden": [],
  "validations": [],
  "relations": {
    "user": {
      "type": "belongsTo",
      "model": "BaseUser",
      "foreignKey": "userId"
    }
  },
  "acls": [],
  "methods": {}
}