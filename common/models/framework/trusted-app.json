{
    "name": "TrustedApp",
    "base": "BaseEntity",
    "strict": true,
    "plural": "TrustedApps",
    "description": "holds trusted application details",
    "options": {
        "validateUpsert": true,
        "isFrameworkModel": true,
        "queryCacheSize": 20000,
        "queryCacheExpiration": 86400000
    },
    "properties": {
        "appId": {
            "type": "string",
            "required": true
        },
        "appName": {
            "type": "string"
        },
        "username": {
            "type": "string"
        },
        "publicKey": {
            "type": "string"
        },
        "expiry": {
            "type": "number"
        },
        "supportedRoles": {
            "type": ["string"]
        }
    },
    "validations": [],
    "relations": {},
    "acls": [{
            "principalType": "ROLE",
            "principalId": "$everyone",
            "permission": "DENY",
            "property": "*",
            "accessType": "*"
        },
        {
            "principalType": "ROLE",
            "principalId": "$authenticated",
            "permission": "ALLOW",
            "property": "*",
            "accessType": "*"
        }
    ],
    "cacheable": true,
    "methods": {}
}