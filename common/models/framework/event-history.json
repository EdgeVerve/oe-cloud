{
    "name": "EventHistory",
    "plural": "EventsHistroy",
    "base": "PersistedModel",
    "strict": true,
    "description": "ensure reliability on failing events on all entities",
    "idInjection": false,
    "properties": {
        "hostName": {
            "type": "string"
        },
        "timestamp": {
            "type": "Date"
        },
        "doStartUpOperation": {
            "type": "boolean"
        },
        "eventHistoryMap": {
            "type": "object"
        }
    },
    "options": {
        "proxyEnabled": true,
        "proxyMethods": [{"name":"createEventHistoryRemote"}]
    }
}
